<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="com.company.lifecycle.Pizza" name="pizza1" init-method="init" destroy-method="destroy">
        <property name="price" value="350"/>
    </bean>
    <bean class="com.company.lifecycle.Burger" name="Burger1">
        <property name="price" value="250"/>
    </bean>
</beans>